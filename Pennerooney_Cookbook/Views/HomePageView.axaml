<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Pennerooney_Cookbook.Views.HomePageView">
    
    <Grid ColumnDefinitions="*, *, *, *" RowDefinitions="*, *, *">
        <Card Grid.Row="0" Grid.Column="0" />
        <Card Grid.Row="0" Grid.Column="1" />
        <Card Grid.Row="0" Grid.Column="2" />
        <Card Grid.Row="0" Grid.Column="3" />
        <Card Grid.Row="1" Grid.Column="0" />
        <Card Grid.Row="1" Grid.Column="1" />
        <Card Grid.Row="1" Grid.Column="2" />
        <Card Grid.Row="1" Grid.Column="3" />
        <Card Grid.Row="2" Grid.Column="0" />
        <Card Grid.Row="2" Grid.Column="1" />
        <Card Grid.Row="2" Grid.Column="2" />
        <Card Grid.Row="2" Grid.Column="3" />
    </Grid>
</UserControl>
